{
  "compilerOptions": {
    "declaration": true,
    "downlevelIteration": true,
    "importHelpers": true,
    "inlineSourceMap": true,
    "lib": [
      "dom",
      "es2015",
      "esnext.asynciterable",
      "scripthost"
    ],
    "module": "commonjs",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "newLine": "LF",
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "strict": true,
    "suppressImplicitAnyIndexErrors": true,
    "target": "es5"
  }
}
